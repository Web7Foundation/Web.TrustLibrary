  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Web7.TrustLibrary.NBitcoin.BIP39\Web7.TrustLibrary.NBitcoin.BIP39.csproj" />
  </ItemGroup>

</Project>
